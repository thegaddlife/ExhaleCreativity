
<div class="sqs-layout sqs-grid-12 columns-12" data-type="page">

  <div class="row sqs-row">

    <div class="col sqs-col-12 span-12">

      <div class="sqs-block horizontalrule-block sqs-block-horizontalrule" data-block-type="47">
        <div class="sqs-block-content"><hr></div>
      </div>

      <div class="sqs-block html-block sqs-block-html" data-block-type="2">

        <div class="sqs-block-content">
          <h1 class="text-align-center">{collection.title}</h1>
          <h2 dir="ltr" class="text-align-center">{collection.description}</h2>
        </div>

      </div>

      <div class="sqs-block horizontalrule-block sqs-block-horizontalrule" data-block-type="47">
        <div class="sqs-block-content"><hr></div>
      </div>

    </div>

  </div>

</div>

<div class="blog-list blog-wrapper autocolumns-wrapper" id="blogList" data-controller="BlogLayout" data-controllers-bound="BlogLayout">

{.repeated section items}

<article class="entry entry--list h-entry has-main-image hentry category-plants category-styling author-laura-nguyen post-type-text article-index-1 autocolumns-item-wrapper" label="Portfolio Item" style="width: 352.667px; top: 0px; left: 0px;">
    <div class="thumbnail-title-wrapper">
      
      <div class="excerpt-thumb">
        <a href="{sourceUrl}" class="excerpt-image content-fill autocolumns-image-wrapper" style="padding-bottom: 66%;">
          <img data-src="{assetUrl}" data-image="{assetUrl}" data-image-dimensions="{originalSize}" data-image-focal-point="0.5,0.5" alt="{title}" class="" src="{assetUrl}?format=2500w" data-image-resolution="2500w" style="">
          <div class="thumb-overlay"></div>
        </a>
      </div>
      
    </div>

    <div class="entry-text">  

      <header class="entry-header">
        <time class="dt-published date-callout" datetime="2016-02-12" pubdate="">
          <div class="date-wrapper">
            <span class="month">Feb </span><span class="day">12</span>
          </div>
        </time>
        <h2 class="entry-title entry-title--list p-name" itemprop="headline" data-content-field="title">
          
          <a href="{sourceUrl}" class="u-url" rel="bookmark">
            <time class="dt-published date-highlight" datetime="2016-02-12" pubdate="">Feb '17 </time>
            {title}
          </a>
        </h2>
      </header>
      
      <div class="entry-content">
        <div class="blog-meta-item blog-meta-author"><a href={customContent.memberUrl} class="blog-author-name">{customContent.memberName}</a> </div>
        <div class="blog-meta-item blog-categories"><a href="/?category=Plants" class="blog-category blog-category--Plants">Plants</a><span class="blog-meta-delimiter">, </span><a href="/?category=Styling" class="blog-category blog-category--Styling">Styling</a></div>
        <div class="entry-excerpt p-summary">{excerpt}</p></div>        
      </div>

    </div>
  </article>

{.end}

</div>